    "react-dates": "^18.2.2",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.6.3",
    "react-map-gl": "^4.0.2",
    "react-maskedinput": "^4.0.1",
    "react-phone-number-input": "^2.3.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
