    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "object-assign": "4.1.1",
    "popper.js": "^1.14.6",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.2",
    "query-string": "^6.2.0",
    "r-dom": "^2.4.0",
    "raf": "3.4.1",
